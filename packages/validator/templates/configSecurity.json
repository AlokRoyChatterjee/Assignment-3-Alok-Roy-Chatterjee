{
  "config": {
    "ois": {
      "__arrayItem": {
        "__oisSpecs": {}
      }
    },
    "triggers": {
      "__optional": {
        "request": {
          "__arrayItem": {
            "endpointId": {
              "__regexp": "^0x[a-fA-F0-9]{40}$"
            },
            "oisTitle": {},
            "endpointName": {}
          }
        }
      }
    },
    "nodeSettings": {
      "providerIdShort": {
        "__regexp": "^[a-fA-F0-9]{7}$"
      },
      "nodeVersion": {
        "__regexp": "^([0-9]*\\.){2}([0-9]*)$"
      },
      "cloudProvider": {
        "__regexp": "^aws$"
      },
      "region": {},
      "stage": {},
      "logFormat": {
        "__regexp": "^(json|plain)$"
      },
      "chains": {
        "__arrayItem": {
          "id": {},
          "type": {
            "__regexp": "^evm$"
          },
          "providers": {
            "__arrayItem": {
              "name": {},
              "url": {
                "__regexp": "^(https?|ftp)://[^\\s/$.?#].[^\\s]*$"
              },
              "__optional": {
                "blockHistoryLimit": {},
                "minConfirmations": {}
              }
            }
          },
          "__optional": {
            "providerAdminForRecordCreation": {
              "__regexp": "^0x[a-fA-F0-9]{40}$"
            },
            "contracts": {
              "__keyRegexp": "^(Airnode|Convenience)$",
              "__objectItem": {
                "__regexp": "^0x[a-fA-F0-9]{40}$"
              }
            }
          }
        }
      }
    },
    "id": {
      "__conditions": [
        {
          "__if": {
            "__this": ".*"
          },
          "__rootThen": {
            "security": {
              "id": {
                "__regexp": "^__match$",
                "__level": "error"
              }
            }
          }
        }
      ]
    }
  },
  "security": {
    "__any": {
      "__keyRegexp": "^(?!id).*$",
      "__objectItem": {
        "__arrayItem": {
          "securitySchemeName": {},
          "value": {}
        }
      }
    },
    "id": {}
  }
}