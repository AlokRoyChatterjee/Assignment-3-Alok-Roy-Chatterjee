{
  "config": {
    "ois": {
      "__arrayItem": {
        "__oisSpecs": {},
        "apiSpecifications": {
          "security": {
            "__conditions": [
              {
                "__if": {
                  "__this_name": ".*"
                },
                "__rootThen": {
                  "security": {
                    "apiCredentials": {
                      "[[/config.{{1}}.title]]": {
                        "__any": {
                          "securitySchemeName": {
                            "__regexp": "^__match$"
                          }
                        }
                      }
                    }
                  }
                }
              }
            ]
          }
        },
        "title": {
          "__conditions": [
            {
              "__if": {
                "__this": ".*"
              },
              "__rootThen": {
                "security": {
                  "apiCredentials": {
                    "__match": {}
                  }
                }
              }
            }
          ]
        }
      }
    },
    "triggers": {
      "__optional": {
        "request": {
          "__arrayItem": {
            "endpointId": {
              "__regexp": "^0x[a-fA-F0-9]{40}$"
            },
            "oisTitle": {},
            "endpointName": {}
          }
        }
      }
    },
    "nodeSettings": {
      "providerIdShort": {
        "__regexp": "^[a-fA-F0-9]{7}$"
      },
      "nodeVersion": {
        "__regexp": "^([0-9]*\\.){2}([0-9]*)$"
      },
      "cloudProvider": {
        "__regexp": "^(aws|local)$"
      },
      "region": {},
      "stage": {},
      "logFormat": {
        "__regexp": "^(json|plain)$"
      },
      "chains": {
        "__arrayItem": {
          "id": {},
          "type": {
            "__regexp": "^evm$"
          },
          "providers": {
            "__arrayItem": {
              "name": {},
              "url": {
                "__regexp": "^(https?|ftp)://[^\\s/$.?#].[^\\s]*$"
              },
              "__optional": {
                "blockHistoryLimit": {},
                "minConfirmations": {}
              }
            }
          },
          "contracts": {
            "Airnode": {
              "__regexp": "^0x[a-fA-F0-9]{40}$"
            },
            "Convenience": {
              "__regexp": "^0x[a-fA-F0-9]{40}$"
            }
          },
          "__optional": {
            "providerAdminForRecordCreation": {
              "__regexp": "^0x[a-fA-F0-9]{40}$"
            }
          }
        }
      }
    },
    "id": {
      "__conditions": [
        {
          "__if": {
            "__this": ".*"
          },
          "__rootThen": {
            "security": {
              "id": {
                "__regexp": "^__match$",
                "__level": "error"
              }
            }
          }
        }
      ]
    }
  },
  "security": {
    "apiCredentials": {
      "__conditions": [
        {
          "__if": {
            "__this_name": ".*"
          },
          "__rootThen": {
            "config": {
              "ois": {
                "__any": {
                  "title": {
                    "__regexp": "^__match$"
                  }
                }
              }
            }
          }
        }
      ],
      "__arrayItem": {
        "__conditions": [
          {
            "__if": {
              "securitySchemeName": ".*"
            },
            "__rootThen": {
              "config": {
                "ois": {
                  "__any": {
                    "title": {
                      "__regexp": "^{{2}}$"
                    },
                    "security": {
                      "__match": {}
                    }
                  }
                }
              }
            }
          }
        ],
        "securitySchemeName": {},
        "value": {}
      }
    },
    "id": {}
  }
}